{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern stack",
  "main": "app.js",
  "scripts": {
    "install": "npm install --prefix client npm run build --prefix client",
    "dev": "node app.js",
    "server": "cross-env NODE_ENV=development nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "mern"
  ],
  "author": "Dilrozbek Raximov <raximvodilrozbek@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.11",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.13"
  }
}
